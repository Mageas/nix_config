super + Return
	alacritty

super + d
	~/.config/rofi/launcher

alt + Return
	emacsclient -c -a 'emacs'

super + e
	alacritty -e "joshuto_window"

alt + F1
	~/.config/rofi/screenshot

alt + F2
	~/.config/rofi/video

alt + F3
	~/.config/rofi/task_manager

alt + F12
	~/.config/rofi/powermenu

F10
	dwm-volume toggle; kill -44 $(pidof dwmblocks)

F11
	dwm-volume set=-2%; kill -44 $(pidof dwmblocks)

F12
	dwm-volume set=+2%; kill -44 $(pidof dwmblocks)